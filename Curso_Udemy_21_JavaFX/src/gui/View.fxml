<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <ComboBox fx:id="comboBoxPerson" layoutX="22.0" layoutY="27.0" onAction="#onComboBoxAction" prefWidth="150.0" promptText="Select a person" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="10.0" />
      <Button fx:id="btAll" layoutX="222.0" layoutY="27.0" mnemonicParsing="false" onAction="#onBtAllAction" text="Show All" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="10.0" />
   </children>
</AnchorPane>
